    "build-prod-android": "cp ./.env.production ./.env && npx react-native run-android",
    "push-bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "postinstall": "patch-package"
  },
  "dependencies": {
