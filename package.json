{
  "name": "mobileApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "build-dev-ios": "cp ./.env.development ./.env && npx react-native run-ios",
    "build-qa-ios": "cp ./.env.staging ./.env && npx react-native run-ios",
